//UNLOAD EXEC PGM=IRRDBU00,PARM=LOCK
//INDD1 DD DSN=SYS1.RACFDS.BACKUP,DISP=SHR
//OUTDD DD DSN=&&INT,VOL=SER=ZUSER1,
