//SYSPRINT DD SYSOUT=*
//*
//*        CLEANUP
//*
//SUBSET    EXEC PGM=IEFBR14
//SORTOF1   DD   DSN=USER02.REG200,DISP=(MOD,DELETE),
//          UNIT=(3390),SPACE=(CYL,(1,10),RLSE)
//SORTOF2   DD   DSN=USER02.REG220,DISP=(MOD,DELETE),
//          UNIT=(3390),SPACE=(CYL,(1,10),RLSE)
//SORTOF3   DD   DSN=USER02.REG270,DISP=(MOD,DELETE),
